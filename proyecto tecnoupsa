//Funciones


void Entrenador::cargarDatosEntrenador(TextBox^ tBnombreE, TextBox^ tBapellidoE,TextBox^ tBedadE, TextBox^ tBtelefonoE, TextBox^ tBcorreoE, TextBox^ tBrutinaD, TextBox^ tBrutinaT, TextBox^ tBrutinaN, TextBox^ tBidE){
		string a =  marshal_as< std::string>(tBnombreE -> Text);
	    strcpy_s(nombre,(a.c_str()));
		string b =  marshal_as< std::string>(tBapellidoE -> Text);
		strcpy_s(apellido,(b.c_str()));
		setEdad(System::Convert::ToInt32(tBedadE -> Text));
		setId(System::Convert::ToInt32(tBidE -> Text));
		string c =  marshal_as< std::string>(tBtelefonoE -> Text);
		strcpy_s(telefono,(c.c_str()));
		string d =  marshal_as< std::string>(tBcorreoE -> Text);
		strcpy_s(correo,(d.c_str()));
		string e =  marshal_as< std::string>(tBrutinaD -> Text);
		strcpy_s(rutinaD,(e.c_str()));
		string f =  marshal_as< std::string>(tBrutinaT -> Text);
		strcpy_s(rutinaT,(f.c_str()));
		string g =  marshal_as< std::string>(tBrutinaN -> Text);
		strcpy_s(rutinaN,(g.c_str()));
		

}

void Entrenador::guardarArchivo(){
	
	FILE*ptr;
	ptr = fopen("entrenador.dat","a+b");
	fwrite(&*this,sizeof(*this),1,ptr);
	fclose(ptr);

}

void Entrenador::entrenadorAgrilla(DataGridView^ grilla,int id, string nombre, string apellido, int edad, string telefono, string correo, int contador){
        grilla -> RowCount = 40;
		grilla -> ColumnCount = 6;
		grilla -> Rows[contador] -> Cells[0] -> Value = System::Convert::ToString(id);
		grilla -> Rows[contador] -> Cells[1] -> Value = marshal_as<System::String^>(nombre);
		grilla -> Rows[contador] -> Cells[2] -> Value = marshal_as<System::String^>(apellido);
		grilla -> Rows[contador] -> Cells[3] -> Value = System::Convert::ToString(edad);
		grilla -> Rows[contador] -> Cells[4] -> Value = marshal_as<System::String^>(telefono);
		grilla -> Rows[contador] -> Cells[5] -> Value = marshal_as<System::String^>(correo);

}

void Entrenador::limpiarGrilla(DataGridView^ grilla){
	    grilla -> RowCount = 1;
		grilla -> ColumnCount = 6;
}

void Entrenador::mostrarDatosEntrenador(DataGridView^ grilla){
	limpiarGrilla(grilla);
	FILE*ptr;
	int contador = 0;
	ptr = fopen("entrenador.dat","rb"); 
	//fopen_s(&ptr,"entrenador.dat","r+b"); watafuck
	if(ptr!=nullptr){
		fread(&*this,sizeof(*this),1,ptr);
		while(feof(ptr) == false){
			entrenadorAgrilla(grilla,getId(),getNombre(),getApellido(),getEdad(),getTelefono(),getCorreo(),contador);
			contador++;
			fread(&*this,sizeof(*this),1,ptr);
		}	
	}
	fclose(ptr);
}



bool Entrenador::verificarSiExiste(int id){
	int i=0;
	FILE *ptr;
	ptr=fopen("entrenador.dat","rb");
	if(ptr!=NULL){ 
		fread(&*this, sizeof(*this), 1, ptr);
		while (feof(ptr)==false) {
			if(getId()==id)
			{
				fclose(ptr);
				return true;
			}
			fread (&*this, sizeof(*this), 1, ptr);
		}
	}
	fclose(ptr);
	return false;
}
