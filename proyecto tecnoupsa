	id = A;
}

void Nutricionista::setSeleccion(int A){
    seleccion = A;
}

void Nutricionista::setTelefono(char *A){
	strcat(telefono,A);
}

void Nutricionista::setCorreo(char *A){
	strcat(correo,A);
