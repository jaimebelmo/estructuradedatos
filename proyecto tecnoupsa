private: System::Void button1_Click(System::Object^  sender, System::EventArgs^  e) {

			comboNutricionistas->Items->Clear();
			comboEntrenadores->Items->Clear();
	
			 Nutricionista N;
			string nombre;
			string apellido;
			string completo;
			    FILE *ptr;
				ptr = fopen("nutricionista.dat","rb");
				if(ptr!=NULL){
		
						fread(&N, sizeof(N), 1, ptr);
						while (feof(ptr)==false){	
				
							nombre = N.getNombre();
							apellido =N.getApellido();
							completo = nombre + " " + apellido;
							comboNutricionistas->Items->Add(marshal_as<System::String^>(completo));
							
							fread(&N, sizeof(N), 1, ptr);
				
				        }
						  
				}else{
					MessageBox::Show("Algo salio mal con los archivos, revisad");
					fclose(ptr);  
				}
				
			Entrenador E;
			string nombreB;
			string apellidoB;
			string completoB;
				 FILE *ptrB;
				ptrB = fopen("entrenador.dat","rb");
				if(ptrB!=NULL){
					fread(&E, sizeof(E), 1, ptrB);
						while (feof(ptrB)==false){	
							nombreB = E.getNombre();
							apellidoB = E.getApellido();
							completoB = nombreB + " " + apellidoB;
							comboEntrenadores->Items->Add(marshal_as<System::String^>(completoB));
							fread(&E, sizeof(E), 1, ptrB);
					    }

				}else{
					MessageBox::Show("Algo salio mal con los archivos, revisad");
					fclose(ptrB);
				    
				}
		 }
private: System::Void button3_Click(System::Object^  sender, System::EventArgs^  e) {


			 Nutricionista N;
			 String^ seleccion = comboNutricionistas->SelectedItem->ToString();
						
			string cadena =  marshal_as< std::string>(seleccion);
            int posicionC = cadena.find(" ");
            string primeraParte  = cadena.substr(0, posicionC);
			string cortada = cadena.substr(posicionC+1,cadena.length());
			FILE *ptr;
			ptr = fopen("nutricionista.dat","rb");
			if(ptr!=NULL){
				fread(&N,sizeof(N),1,ptr);
				while(feof(ptr)==false){
					if(primeraParte == N.getNombre()){
						if(cortada == N.getApellido()){
							grillaNutricionistas -> RowCount = 3;
							grillaNutricionistas -> ColumnCount = 3;
							grillaNutricionistas -> Rows[0] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getDietaD());
							grillaNutricionistas -> Rows[1] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getDietaT());
							grillaNutricionistas -> Rows[2] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getDietaN());
							grillaNutricionistas -> Rows[1] -> Cells[1] -> Value =  marshal_as<System::String^>(N.getTelefono());
							grillaNutricionistas -> Rows[1] -> Cells[2] -> Value =  marshal_as<System::String^>(N.getCorreo());

						}
					}
					fread(&N,sizeof(N),1,ptr);
				}
			}
		 }
