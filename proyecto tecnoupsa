	private: System::Void PantallaNutricionista_Load(System::Object^  sender, System::EventArgs^  e) {
			 }
	private: System::Void button1_Click(System::Object^  sender, System::EventArgs^  e) {
				 Nutricionista N;
				 Validaciones V;
				 string n=marshal_as<std::string>(tBnombreN->Text);
		    	 string ap=marshal_as<std::string>(tBapellidoN->Text);
			     string edad=marshal_as<std::string>(tBedadN->Text);
		 	     string id=marshal_as<std::string>(tBidN->Text);
		    	 string t=marshal_as<std::string>(tBtelefonoN->Text);
	             string c=marshal_as<std::string>(tBcorreoN->Text);
		    	 string aa = marshal_as<std::string>(tBdietaD->Text);
		    	 string ab = marshal_as<std::string>(tBdietaT->Text);
		    	 string ac = marshal_as<std::string>(tBdietaN->Text);
				   if(n.size() <1 || ap.size() <1 || edad.size() < 1 || t.size()<1 || c.size() < 1 || aa.size() <1 || ab.size() <1 || ac.size() <1 || id.size() < 1){
			     MessageBox::Show("Alguno de los datos Ingresados Esta Vacio");
			       }else{
					   if((V.soloLetras(n) == true) && (V.soloLetras(ap) == true) && (V.soloNumeros(edad)==true)&&(V.soloNumeros(t)==true)&&(V.verificarEmail(tBcorreoN)==true) && (V.soloLetras(aa) == true)  && (V.soloLetras(ab) == true)  && (V.soloLetras(ac) == true) && (V.soloNumeros(id)==true) ){
				 N.cargarDatosNutricionista(tBidN,tBnombreN, tBapellidoN , tBedadN,tBtelefonoN,  tBcorreoN,  tBdietaD, tBdietaT,  tBdietaN);
			     N.guardarArchivo();
					   }else{
						    MessageBox::Show("Alguno de los campos no esta con los datos correctos");
					   }
				   }
			 }
private: System::Void textBox3_TextChanged(System::Object^  sender, System::EventArgs^  e) {
		 }
private: System::Void button2_Click(System::Object^  sender, System::EventArgs^  e) {
			  Nutricionista N;
			  N.mostrarDatosNutricionista(grilla);
		 }
private: System::Void label4_Click(System::Object^  sender, System::EventArgs^  e) {
		 }
};
}

