cliente.h


#pragma once
#include <iostream>
#include <stdio.h>
#include <fstream>
#include "conio.h"
#include <string>
#include <string.h>
#include <msclr\marshal_cppstd.h>
 using namespace std;
 using namespace System::Windows::Forms;
 using namespace msclr::interop;
const int TAM=40;
class Cliente
{
private:
	
	char nombre[TAM];
	char apellido[TAM];
	int edad;
	char telefono[TAM];;
	int id;
	double peso;
	double estatura;

public:
	Cliente(void);
	void setNombre(char *A);
	void setApellido(char *A);
	void setEdad(int A);
	void setId(int A);
	void setTelefono(char *A);
	void setPeso(double A);
	void setEstatura(double A);

	string getNombre();
	string getApellido();
	int getEdad();
	int getId();
	string getTelefono();
	double getPeso();
	double getEstatura();

	//Funciones 
	
	void cargarDatosCliente(TextBox^ id, TextBox^ nombre, TextBox^ apellido,TextBox^ edad, TextBox^ telefono, TextBox^ peso, TextBox^ estatura);
	void mostrarDatosCliente(DataGridView^ grilla);
	void guardarArchivo();
    void clienteAgrilla(DataGridView^ grilla,int id, string nombre, string apellido, int edad, string telefono, double peso, double estatura, int contador);
	void limpiarGrilla(DataGridView^ grilla);
	bool verificarSiExiste(int id);
	
};
entrenador.h


#pragma once
#include <iostream>
#include <stdio.h>
#include <fstream>
#include "conio.h"
#include <string>
#include <string.h>
#include <msclr\marshal_cppstd.h>
const int AA = 40;
const int Aa = 120;
 using namespace std;
 using namespace System::Windows::Forms;
 using namespace msclr::interop;
class Entrenador
{

private:
    char nombre[AA];
	char apellido[AA];
	int edad;
	int id;
	int seleccion; //Se usa para el selected Item desde el form
	char telefono[AA];
	char correo[AA];
	char rutinaD[Aa];
	char rutinaT[Aa];
	char rutinaN[Aa];
	
public:
	Entrenador(void);
	void setNombre(char *A);
	void setApellido(char *A);
	void setEdad(int A);
	void setSeleccion(int A);
	void setId(int A);
	void setTelefono(char *A);
	void setCorreo(char *A);
	void setRutinaD(char *A);
	void setRutinaT(char *A);
	void setRutinaN(char *A);

	string getNombre();
	string getApellido();
	int getEdad();
	int getId();
	int getSeleccion();
	string getTelefono();
	string getCorreo();
	string getRutinaD();
	string getRutinaT();
	string getRutinaN();

	//Funciones

	void cargarDatosEntrenador(TextBox^ nombre, TextBox^ apellido,TextBox^ edad, TextBox^ telefono, TextBox^ correo, TextBox^ rutinaD, TextBox^ rutinaT, TextBox^ rutinaN, TextBox^ tBidE);
	void mostrarDatosEntrenador(DataGridView^ grilla);
	void guardarArchivo();
    void entrenadorAgrilla(DataGridView^ grilla,int id, string nombre, string apellido, int edad, string telefono, string correo, int contador);
	void limpiarGrilla(DataGridView^ grilla);
	bool verificarSiExiste(int id);
};

nutricioniste.h
#pragma once

#include <iostream>
#include <stdio.h>
#include <fstream>
#include "conio.h"
#include <string>
#include <string.h>
#include <msclr\marshal_cppstd.h>
 using namespace std;
 using namespace System::Windows::Forms;
 using namespace msclr::interop;

const int MAX = 40;
const int MAXB = 120;
class Nutricionista
{
private:
	char nombre[MAX];
	char apellido[MAX];
	int edad;
	int id;
	int seleccion;
	char telefono[MAX];
	char correo[MAX];
	char dietaD[MAXB];
	char dietaT[MAXB];
	char dietaN[MAXB];

public:

	Nutricionista(void);
	void setNombre(char* A);
	void setApellido(char *A);
	void setEdad(int A);
	void setId(int A);
	void setSeleccion(int A);
	void setTelefono(char *A);
	void setCorreo(char *A);
	void setDietaD(char *A);
	void setDietaT(char *A);
	void setDietaN(char *A);
	

	string getNombre();
	string getApellido();
	int getEdad();
	int getId();
	int getSeleccion();
	string getTelefono();
	string getCorreo();
	string getDietaD();
	string getDietaT();
	string getDietaN();

	//Funciones

	void cargarDatosNutricionista(TextBox^ id,TextBox^ nombre, TextBox^ apellido,TextBox^ edad, TextBox^ telefono, TextBox^ correo, TextBox^ dietaD, TextBox^ dietaT, TextBox^ dietaN);
    void mostrarDatosNutricionista(DataGridView^ grilla);
    void guardarArchivo();
    void nutricionistaAgrilla(DataGridView^ grilla,int id, string nombre, string apellido, int edad, string telefono, string correo, int contador);
    void limpiarGrilla(DataGridView^ grilla);
	bool verificarSiExiste(int id);
};

cliente.cpp


#include "StdAfx.h"
#include "Cliente.h"
#include <iostream>
#include "conio.h"
#include <string>
#include <msclr\marshal_cppstd.h>

using namespace System::Windows::Forms;
using namespace std;
using namespace msclr::interop;
Cliente::Cliente(void)
{
	
	edad = 0;
	peso = 0.00;
	estatura = 0.00;
	id =0;
}

void Cliente::setNombre(char *A){
	strcat(nombre,A);
}

void Cliente::setApellido(char *A){
	strcat(apellido,A);
}

void Cliente::setEdad(int A){
	edad = A;
}

void Cliente::setId(int A){
	id = A;
}

void Cliente::setTelefono(char *A){
	strcat(telefono,A);
}

void Cliente::setPeso(double A){
	peso = A;
}

void Cliente::setEstatura(double A){
	estatura = A;
}

string Cliente::getNombre(){
	string A(nombre);
	return A;
}

string Cliente::getApellido(){
	string A(apellido);
	return A;
}

int Cliente::getEdad(){
	return edad;
}

int Cliente::getId(){
    return id;
}

string Cliente::getTelefono(){
	return telefono;
}

double Cliente::getPeso(){
	return peso;
}

double Cliente::getEstatura(){
	return estatura;
}

//Funciones 

void Cliente::cargarDatosCliente(TextBox^ tBid,TextBox^ tBnombre, TextBox^ tBapellido, TextBox^ tBedad, TextBox^ tBtelefono, TextBox^ tBpeso, TextBox^ tBestatura){
		string a =  marshal_as< std::string>(tBnombre -> Text);
	    strcpy_s(nombre,(a.c_str()));
		string b =  marshal_as< std::string>(tBapellido -> Text);
		strcpy_s(apellido,(b.c_str()));
		setEdad(System::Convert::ToInt32(tBedad -> Text));
		setId(System::Convert::ToInt32(tBid -> Text));
		string c =  marshal_as< std::string>(tBtelefono -> Text);
		strcpy(telefono,(c.c_str()));
		setPeso(System::Convert::ToDouble(tBpeso -> Text));
		setEstatura(System::Convert::ToDouble(tBestatura -> Text));

		

}

void Cliente::guardarArchivo(){
	FILE*ptr;	
	ptr = fopen("cliente.dat","a+b");
	fwrite(&*this,sizeof(*this),1,ptr);
	fclose(ptr);
}

void Cliente::clienteAgrilla(DataGridView^ grilla, int id,string nombre, string apellido, int edad, string telefono, double peso, double estatura, int contador){
        grilla -> RowCount = 40;
		grilla -> ColumnCount = 7;
		grilla -> Rows[contador] -> Cells[0] -> Value = System::Convert::ToString(id);
		grilla -> Rows[contador] -> Cells[1] -> Value = marshal_as<System::String^>(nombre);
		grilla -> Rows[contador] -> Cells[2] -> Value = marshal_as<System::String^>(apellido);
		grilla -> Rows[contador] -> Cells[3] -> Value = System::Convert::ToString(edad);
		grilla -> Rows[contador] -> Cells[4] -> Value = marshal_as<System::String^>(telefono);
		grilla -> Rows[contador] -> Cells[5] -> Value = System::Convert::ToString(peso);
	 	grilla -> Rows[contador] -> Cells[6] -> Value = System::Convert::ToString(estatura);
}




void Cliente::limpiarGrilla(DataGridView^ grilla){
	    grilla -> RowCount = 1;
		grilla -> ColumnCount = 7;
}

void Cliente::mostrarDatosCliente(DataGridView^ grilla){
	limpiarGrilla(grilla);
	FILE*ptr;
	int contador = 0;
	
	ptr = fopen("cliente.dat","rb");
	if(ptr!=nullptr){
		fread(&*this,sizeof(*this),1,ptr);
		while(feof(ptr) == false){
			clienteAgrilla(grilla,getId(),getNombre(),getApellido(),getEdad(),getTelefono(),getPeso(),getEstatura(),contador);
			contador++;
			fread(&*this,sizeof(*this),1,ptr);
		}	
	}
	fclose(ptr);
}

bool Cliente::verificarSiExiste(int id){
	int i=0;
	FILE *ptr;
	ptr=fopen("cliente.dat","rb");
	if(ptr!=NULL){ 
		fread(&*this, sizeof(*this), 1, ptr);
		while (feof(ptr)==false) {
			if(getId()==id)
			{
				fclose(ptr);
				return true;
			}
			fread (&*this, sizeof(*this), 1, ptr);
		}
	}
	fclose(ptr);
	return false;
}

