		 }
		  private: System::Void PantallaEntrenador_Load(System::Object^  sender, System::EventArgs^  e) {
				   }
private: System::Void btnInsertar_Click(System::Object^  sender, System::EventArgs^  e) {
 
			 Entrenador E;
			 Validaciones V;
			 string n=marshal_as<std::string>(tBnombreE->Text);
			 string ap=marshal_as<std::string>(tBapellidoE->Text);
			 string edad=marshal_as<std::string>(tBedadE->Text);
			 string id=marshal_as<std::string>(tBidE->Text);
			 string t=marshal_as<std::string>(tBtelefonoE->Text);
	         string c=marshal_as<std::string>(tBcorreoE->Text);
			 string aa = marshal_as<std::string>(tBrutinaD->Text);
			 string ab = marshal_as<std::string>(tBrutinaT->Text);
			 string ac = marshal_as<std::string>(tBrutinaN->Text);
			  if(n.size() <1 || ap.size() <1 || edad.size() < 1 || t.size()<1 || c.size() < 1 || aa.size() <1 || ab.size() <1 || ac.size() <1 || id.size() < 1){
			     MessageBox::Show("Alguno de los datos Ingresados Esta Vacio");
			 }else{
				 if((V.soloLetras(n) == true) && (V.soloLetras(ap) == true) && (V.soloNumeros(edad)==true)&&(V.soloNumeros(t)==true)&&(V.verificarEmail(tBcorreoE)==true) && (V.soloLetras(aa) == true)  && (V.soloLetras(ab) == true)  && (V.soloLetras(ac) == true) && (V.soloNumeros(id)==true) ){
			         E.cargarDatosEntrenador(tBnombreE, tBapellidoE, tBedadE, tBtelefonoE,tBcorreoE,tBrutinaD,tBrutinaT,tBrutinaN,tBidE);
			         E.guardarArchivo();
				 }
			  }
		 }
private: System::Void button1_Click(System::Object^  sender, System::EventArgs^  e) {
			 Entrenador E;
			 E.mostrarDatosEntrenador(grilla);
		 }
private: System::Void tBrutinaC_TextChanged(System::Object^  sender, System::EventArgs^  e) {
		 }
private: System::Void grilla_CellContentClick(System::Object^  sender, System::Windows::Forms::DataGridViewCellEventArgs^  e) {
		 }
};
}

