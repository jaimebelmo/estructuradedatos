		 }
private: System::Void button2_Click(System::Object^  sender, System::EventArgs^  e) {

            Entrenador N;		 
			String^ seleccion = comboEntrenadores->SelectedItem->ToString();
					
			string cadena =  marshal_as< std::string>(seleccion);
            int posicionC = cadena.find(" ");
            string primeraParte  = cadena.substr(0, posicionC);
			string cortada = cadena.substr(posicionC+1,cadena.length());
			FILE *ptr;
			ptr = fopen("entrenador.dat","rb");
			if(ptr!=NULL){
				fread(&N,sizeof(N),1,ptr);
				while(feof(ptr)==false){
					if(primeraParte == N.getNombre()){
						if(cortada == N.getApellido()){
							grillaEntrenadores -> RowCount = 3;
							grillaEntrenadores -> ColumnCount = 3;
							grillaEntrenadores -> Rows[0] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getRutinaD());
							grillaEntrenadores -> Rows[1] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getRutinaT());
							grillaEntrenadores -> Rows[2] -> Cells[0] -> Value =  marshal_as<System::String^>(N.getRutinaN());
							grillaEntrenadores -> Rows[1] -> Cells[1] -> Value =  marshal_as<System::String^>(N.getTelefono());
							grillaEntrenadores -> Rows[1] -> Cells[2] -> Value =  marshal_as<System::String^>(N.getCorreo());

						}
					}
					fread(&N,sizeof(N),1,ptr);
				}
			}			 

		 }
private: System::Void Seleccion_Load(System::Object^  sender, System::EventArgs^  e) {
		 }
