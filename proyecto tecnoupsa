	private: System::Void PantallaCliente_Load(System::Object^  sender, System::EventArgs^  e) {
			 }

		
private: System::Void button1_Click(System::Object^  sender, System::EventArgs^  e) {

			 
			 Cliente C;
			 Validaciones V;
			 string n=marshal_as<std::string>(tBnombre->Text);
			 string ap=marshal_as<std::string>(tBapellido->Text);
			 string edad=marshal_as<std::string>(tBedad->Text);
			 string t=marshal_as<std::string>(tBtelefono->Text);
	         string p=marshal_as<std::string>(tBpeso->Text);
			 string o=marshal_as<std::string>(tBestatura->Text);
			 string id = marshal_as<std::string>(tBid->Text);
			 if(n.size() <1 || ap.size() <1 || edad.size() < 1 || t.size()<1 || p.size() < 1 || o.size() <1 || id.size() < 1){
			     MessageBox::Show("Alguno de los datos Ingresados Esta Vacio");
			 }else{
				 if((V.soloLetras(n) == true) && (V.soloLetras(ap) == true) && (V.soloNumeros(edad)==true)&&(V.soloNumeros(t)==true) && (V.verificarDecimal(tBpeso) == true) && (V.verificarDecimal(tBestatura)==true)&& (V.soloNumeros(id)==true)  )  {
					  C.cargarDatosCliente(tBid,tBnombre,tBapellido,tBedad,tBtelefono,tBpeso,tBestatura);
		              C.guardarArchivo();
		    
				 }else{
					  
					  MessageBox::Show("Alguno de los datos Ingresados tiene errores");
				 }
			 }
	
		 }
		 
private: System::Void textBox4_TextChanged(System::Object^  sender, System::EventArgs^  e) {
		 }
private: System::Void btnMostrar_Click(System::Object^  sender, System::EventArgs^  e) {
			
			Cliente C;
			C.mostrarDatosCliente(grilla);
		 }
private: System::Void tBpeso_TextChanged(System::Object^  sender, System::EventArgs^  e) {
		 }
private: System::Void tBnombre_TextChanged(System::Object^  sender, System::EventArgs^  e) {
		 }
};
}

