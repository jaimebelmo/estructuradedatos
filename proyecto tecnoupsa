//Funciones

void Nutricionista::cargarDatosNutricionista(TextBox^ tBidN, TextBox^ tBnombreN, TextBox^ tBapellidoN ,TextBox^ tBedadN, TextBox^ tBtelefonoN, TextBox^ tBcorreoN, TextBox^ tBdietaD, TextBox^ tBdietaT, TextBox^ tBdietaN){
	    string a =  marshal_as< std::string>(tBnombreN -> Text);
	    strcpy_s(nombre,(a.c_str()));
		string b =  marshal_as< std::string>(tBapellidoN -> Text);
		strcpy_s(apellido,(b.c_str()));
		setEdad(System::Convert::ToInt32(tBedadN -> Text));
		setId(System::Convert::ToInt32(tBidN -> Text));
		string c =  marshal_as< std::string>(tBtelefonoN -> Text);
		strcpy_s(telefono,(c.c_str()));
		string d =  marshal_as< std::string>(tBcorreoN -> Text);
		strcpy_s(correo,(d.c_str()));
		string e =  marshal_as< std::string>(tBdietaD -> Text);
		strcpy_s(dietaD,(e.c_str()));
		string f =  marshal_as< std::string>(tBdietaT -> Text);
		strcpy_s(dietaT,(f.c_str()));
		string g =  marshal_as< std::string>(tBdietaN -> Text);
		strcpy_s(dietaN,(g.c_str()));

		
}

void Nutricionista::mostrarDatosNutricionista(DataGridView^ grilla){
	limpiarGrilla(grilla);
	FILE*ptr;
	int contador = 0;
	
	ptr = fopen("nutricionista.dat","rb");
	if(ptr!=NULL){
		fread(&*this,sizeof(*this),1,ptr);
		while(feof(ptr) == false){
			nutricionistaAgrilla(grilla,getId(),getNombre(),getApellido(),getEdad(),getTelefono(),getCorreo(),contador);
			contador++;
			fread(&*this,sizeof(*this),1,ptr);
		}	
	}
	fclose(ptr);
}

void Nutricionista::guardarArchivo(){
	FILE*ptr;
	ptr = fopen("nutricionista.dat","a+b");
	if(ptr!=NULL){
	fwrite(&*this,sizeof(*this),1,ptr);
	}
	fclose(ptr);
}

void Nutricionista::nutricionistaAgrilla(DataGridView^ grilla, int id, string nombre, string apellido, int edad, string telefono, string correo, int contador){
	grilla -> RowCount = 40;
		grilla -> ColumnCount = 6;
		grilla -> Rows[contador] -> Cells[0] -> Value = System::Convert::ToString(id);
		grilla -> Rows[contador] -> Cells[1] -> Value = marshal_as<System::String^>(nombre);
		grilla -> Rows[contador] -> Cells[2] -> Value = marshal_as<System::String^>(apellido);
		grilla -> Rows[contador] -> Cells[3] -> Value = System::Convert::ToString(edad);
		grilla -> Rows[contador] -> Cells[4] -> Value = marshal_as<System::String^>(telefono);
		grilla -> Rows[contador] -> Cells[5] -> Value = marshal_as<System::String^>(correo);
}
void Nutricionista::limpiarGrilla(DataGridView^ grilla){
	    grilla -> RowCount = 1;
		grilla -> ColumnCount = 6;
}

bool Nutricionista::verificarSiExiste(int id){
	int i=0;
	FILE *ptr;
	ptr=fopen("nutricionista.dat","rb");
	if(ptr!=NULL){ 
		fread(&*this, sizeof(*this), 1, ptr);
		while (feof(ptr)==false) {
			if(getId()==id)
			{
				fclose(ptr);
				return true;
			}
			fread (&*this, sizeof(*this), 1, ptr);
		}
	}
	fclose(ptr);
	return false;
}

pantalla.entrenador

#pragma once
#include "Entrenador.h"
#include "Validaciones.h"
namespace Proyecto4312 {

	using namespace System;
	using namespace System::ComponentModel;
	using namespace System::Collections;
	using namespace System::Windows::Forms;
	using namespace System::Data;
	using namespace System::Drawing;

	/// <summary>
	/// Summary for PantallaEntrenador
	/// </summary>
	public ref class PantallaEntrenador : public System::Windows::Forms::Form
	{
	public:
		PantallaEntrenador(void)
		{
			InitializeComponent();
			//
			//TODO: Add the constructor code here
			//
		}

	protected:
		/// <summary>
		/// Clean up any resources being used.
		/// </summary>
		~PantallaEntrenador()
		{
			if (components)
			{
				delete components;
			}
		}
	private: System::Windows::Forms::Button^  btnInsertar;
	private: System::Windows::Forms::Button^  button1;


	private: System::Windows::Forms::Label^  label3;
	private: System::Windows::Forms::Label^  label4;
	private: System::Windows::Forms::Label^  label5;
	private: System::Windows::Forms::Label^  label6;
	private: System::Windows::Forms::Label^  label;

	private: System::Windows::Forms::TextBox^  tBtelefonoE;


	private: System::Windows::Forms::TextBox^  tBedadE;

	private: System::Windows::Forms::TextBox^  tBapellidoE;
	private: System::Windows::Forms::TextBox^  tBcorreoE;





	private: System::Windows::Forms::Label^  label1;
	private: System::Windows::Forms::DataGridView^  grilla;


	private: System::Windows::Forms::Label^  label8;
	private: System::Windows::Forms::TextBox^  tBnombreE;
	private: System::Windows::Forms::TextBox^  tBrutinaN;

	private: System::Windows::Forms::TextBox^  tBrutinaT;

	private: System::Windows::Forms::TextBox^  tBrutinaD;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  id;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  Nombre;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  Apellido;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  Edad;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  Telefono;
	private: System::Windows::Forms::DataGridViewTextBoxColumn^  Email;
	private: System::Windows::Forms::Label^  label2;
	private: System::Windows::Forms::TextBox^  tBidE;
	private: System::Windows::Forms::Label^  label7;









	protected: 

	protected: 

	private:
		/// <summary>
		/// Required designer variable.
		/// </summary>
		System::ComponentModel::Container ^components;

#pragma region Windows Form Designer generated code
		/// <summary>
		/// Required method for Designer support - do not modify
		/// the contents of this method with the code editor.
		/// </summary>
		void InitializeComponent(void)
		{
			this->btnInsertar = (gcnew System::Windows::Forms::Button());
			this->button1 = (gcnew System::Windows::Forms::Button());
			this->label3 = (gcnew System::Windows::Forms::Label());
			this->label4 = (gcnew System::Windows::Forms::Label());
			this->label5 = (gcnew System::Windows::Forms::Label());
			this->label6 = (gcnew System::Windows::Forms::Label());
